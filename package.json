{
  "name": "genshin-artifact-builds",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "gcsim": "npx tsx scripts/gcsim.ts",
    "genshin-db": "npm install --save-dev genshin-db@latest",
    "gen": "npm run genshin-db && npm run scripts && npm run proto && npm run proto-esm",
    "scripts": "node scripts/index.mjs",
    "proto2": "pbjs -t static-module -w commonjs -o ./scripts/proto.cjs ./proto/*.proto",
    "proto": "rm -r ./src/genshin && mkdir -p ./src/genshin && protoc --proto_path=./proto --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/genshin ./proto/*.proto",
    "proto-esm": "rm -r ./genshin && mkdir -p ./genshin && protoc --proto_path=./proto --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=importSuffix=.js --ts_proto_out=./genshin ./proto/*.proto",
    "build": "vite build",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "@piotr-cz/redux-persist-idb-storage": "^1.1.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/react": "^7.30.0",
    "@sentry/tracing": "^7.30.0",
    "classnames": "^2.3.2",
    "crypto-js": "^4.1.1",
    "daisyui": "^4.4.2",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-chained-backend": "^4.2.0",
    "i18next-http-backend": "^1.4.4",
    "jwt-decode": "^3.1.2",
    "mathjs": "^11.5.0",
    "percom": "^1.1.3",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-loading": "^2.0.3",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^0.3.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.12",
    "axios": "^1.2.2",
    "cssnano": "^5.1.13",
    "csv-parse": "^5.3.3",
    "genshin-db": "^5.2.1",
    "i18next-resources-to-backend": "^1.1.1",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "protobufjs": "^7.2.6",
    "protobufjs-cli": "^1.1.2",
    "protoc": "^1.1.3",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.137.0",
    "tsx": "^4.16.5",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
